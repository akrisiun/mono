  <ItemGroup>
    <None Update="..\..\Mono\lib\mono\net45\System.Configuration.dll">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="..\..\Mono\lib\mono\net45\System.Web.dll">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <Target Name="PostBuildTask" AfterTargets="Build">
    <Message Condition=" Exists($(LibDir)) " Importance="High" Text="net*    -&gt; $(LibDir)\$(TargetName).exe" />
    <Message Condition=" Exists($(LibDir)) " Importance="High" Text="           $(LibDir)\mscorlib.dll   -&gt;    $(TargetDir)mscorlib.dll" />
    <Copy Condition=" '$(SolutionDir)' == '*Undefined*'" SourceFiles="$(LibDir)\mscorlib.dll" DestinationFiles=" $(TargetDir)mscorlib.dll" SkipUnchangedFiles="false" />
    <Copy Condition=" '$(SolutionDir)' == '*Undefined*'" SourceFiles="$(LibDir)\mscorlib.pdb" DestinationFiles=" $(TargetDir)mscorlib.pdb" SkipUnchangedFiles="false" />
    <Copy Condition=" '$(SolutionDir)' == '*Undefined*'" SourceFiles="$(LibDir)\System.dll" DestinationFiles=" $(TargetDir)System.dll" SkipUnchangedFiles="false" />
    <Copy Condition=" '$(SolutionDir)' == '*Undefined*'" SourceFiles="$(LibDir)\System.pdb" DestinationFiles=" $(TargetDir)System.pdb" SkipUnchangedFiles="false" />
    <Copy Condition=" '$(SolutionDir)' == '*Undefined*'" SourceFiles="$(LibDir)\System.Xml.dll" DestinationFiles=" $(TargetDir)System.Xml.dll" SkipUnchangedFiles="false" />
    <Copy Condition=" '$(SolutionDir)' == '*Undefined*'" SourceFiles="$(LibDir)\System.Xml.pdb" DestinationFiles=" $(TargetDir)System.Xml.pdb" SkipUnchangedFiles="false" />

    <Message Condition=" Exists($(LibDir)) " Importance="High" Text="           $(LibDir)\System.Web.dll -&gt;    $(TargetDir)System.Web.dll" />
    <Copy Condition=" '$(SolutionDir)' != '*Undefined*' And Exists($(LibDir))" SourceFiles="$(TargetDir)\$(TargetName).exe" DestinationFiles=" $(LibDir)\$(TargetName).exe" SkipUnchangedFiles="false" />

    <Copy Condition=" '$(SolutionDir)' == '*Undefined*'" SourceFiles="$(LibDir)\System.Web.dll" DestinationFiles=" $(TargetDir)System.Web.dll" SkipUnchangedFiles="false" />
    <Copy Condition=" '$(SolutionDir)' == '*Undefined*'" SourceFiles="$(LibDir)\System.Web.pdb" DestinationFiles=" $(TargetDir)System.Web.pdb" SkipUnchangedFiles="false" />
    <Message Condition=" Exists($(LibDir)) " Importance="High" Text="           $(LibDir)\System.Configuration.dll -&gt;    $(TargetDir)System.Configuration.dll" />
    <Copy Condition=" '$(SolutionDir)' == '*Undefined*'" SourceFiles="$(LibDir)\System.Configuration.dll" DestinationFiles=" $(TargetDir)System.Configuration.dll" SkipUnchangedFiles="false" />
    <Copy Condition=" '$(SolutionDir)' == '*Undefined*'" SourceFiles="$(LibDir)\System.Configuration.pdb" DestinationFiles=" $(TargetDir)System.Configuration.pdb" SkipUnchangedFiles="false" />
    <Message Condition=" Exists($(LibDir)) " Importance="High" Text="           $(LibDir)\System.Web.dll -&gt;    $(TargetDir)bin\System.Web.dll" />
    <Copy Condition=" '$(SolutionDir)' == '*Undefined*'" SourceFiles="$(LibDir)\System.Web.dll" DestinationFiles=" $(TargetDir)bin\System.Web.dll" SkipUnchangedFiles="false" />
    <Copy Condition=" '$(SolutionDir)' == '*Undefined*'" SourceFiles="$(LibDir)\System.Web.pdb" DestinationFiles=" $(TargetDir)bin\System.Web.pdb" SkipUnchangedFiles="false" />
  </Target>
</Project>